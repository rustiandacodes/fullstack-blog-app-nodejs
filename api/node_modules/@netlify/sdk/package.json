{
  "name": "@netlify/sdk",
  "version": "1.2.0",
  "description": "",
  "type": "module",
  "bin": {
    "ntli": "./dist/bin/index.js",
    "netlify-integration": "./dist/bin/index.js"
  },
  "files": [
    "dist/**/*"
  ],
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/api/netlify.d.ts",
      "default": "./dist/api/netlify.js"
    },
    "./bin": {
      "default": "./dist/bin/index.js"
    },
    "./commands": {
      "default": "./dist/cli/commands/index.js"
    },
    "./cli-utils": {
      "default": "./dist/cli/util.js"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@netlify/functions": "^1.6.0",
    "@netlify/integrations": "^0.6.0",
    "camelcase": "^7.0.1",
    "chalk": "^5.2.0",
    "commander": "^10.0.0",
    "content-engine": "0.0.25",
    "crypto-js": "^4.1.1",
    "esbuild": "^0.17.12",
    "esbuild-plugin-copy": "^2.1.1",
    "fs-extra": "^11.1.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "node-fetch": "^3.3.1",
    "open": "^9.1.0",
    "outdent": "^0.8.0",
    "toml": "^3.0.0",
    "typescript": "^5.0.2",
    "wretch": "^2.5.1",
    "zod": "^3.21.4",
    "zod-metadata": "^1.1.0",
    "zod-to-json-schema": "^3.21.4",
    "zod-validation-error": "^1.3.1"
  },
  "devDependencies": {
    "@netlify/build": "^29.22.1",
    "@types/chance": "^1.1.3",
    "@types/crypto-js": "^4.1.2",
    "@types/fs-extra": "^11.0.1",
    "@types/js-yaml": "^4.0.5",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "4.14.199",
    "@types/node": "^18.15.11",
    "@types/uglify-js": "^3.17.1",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "chance": "^1.1.11",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "graphql": "^16.7.1",
    "json-schema-to-zod": "^1.1.1",
    "publint": "^0.2.0",
    "vitest": "^0.31.0"
  },
  "overrides": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "scripts": {
    "prebuild": "./scripts/bundle-version.js",
    "build": "pnpm prebuild && tsc --project tsconfig.build.json",
    "watch": "tsc --project tsconfig.build.json --watch",
    "lint": "publint",
    "demo": "cd demo && ([ ! -d '../dist' ] || [ ! -d '../node_modules' ] || [ ! -d './node_modules' ]) && pnpm run build:setup || pnpm run build:w-ntli",
    "test": "pnpm run build && vitest",
    "api-docs": "pnpm typedoc --options typedoc.json"
  }
}